{
  "name": "screen-explorer-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ssr": "ng run screen-explorer-client:serve-ssr",
    "build": "ng build",
    "build:stats": "ng build --stats-json",
    "build:prod": "ng build --prod",
    "build:ssr": "ng build --prod && ng run screen-explorer-client:server:production",
    "serve:stats": "webpack-bundle-analyzer dist/screen-explorer-client/browser/stats-es2015.json",
    "serve:prod": "http-server -p 8000 -c-1 dist/screen-explorer-client/browser --proxy http://localhost:8000?",
    "serve:ssr": "node dist/screen-explorer-client/server/main.js",
    "test": "ng test",
    "test:without-watch": "ng test --watch false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prerender": "ng run screen-explorer-client:prerender",
    "analyze": "npm run build:stats && npm run serve:stats",
    "format": "prettier --write \"**/*.{ts,js,css,scss,html,json,md}\"",
    "format:watch": "onchange \"**/*.{ts,js,css,scss,html,json,md}\" -- prettier --write {{changed}}",
    "format:check": "prettier --check \"**/*.{ts,js,css,scss,html,json,md}\"",
    "format:staged": "pretty-quick --staged --pattern \"**/*.{ts,js,css,scss,html,json,md}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.1",
    "@angular/common": "~9.1.1",
    "@angular/compiler": "~9.1.1",
    "@angular/core": "~9.1.1",
    "@angular/forms": "~9.1.1",
    "@angular/platform-browser": "~9.1.1",
    "@angular/platform-browser-dynamic": "~9.1.1",
    "@angular/platform-server": "~9.1.1",
    "@angular/router": "~9.1.1",
    "@angular/service-worker": "~9.1.1",
    "@avivharuzi/ngx-seo": "^1.0.1",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@glidejs/glide": "^3.3.0",
    "@nguniversal/express-engine": "^9.1.0",
    "bootstrap": "^4.4.1",
    "express": "^4.15.2",
    "ngx-bootstrap": "^5.6.1",
    "ngx-glide": "^1.0.1",
    "rxjs": "~6.5.4",
    "spotlight.js": "^0.6.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.7",
    "@angular/cli": "~9.1.1",
    "@angular/compiler-cli": "~9.1.1",
    "@angular/language-service": "~9.1.1",
    "@nguniversal/builders": "^9.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "http-server": "^0.12.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "onchange": "^6.1.0",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.8.3",
    "webpack-bundle-analyzer": "^3.7.0"
  }
}
